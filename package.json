{
	"name": "weekly-planner",
	"version": "0.0.1",
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"start": "npx sandbox"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"rollup": "^2.40.0",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.0",
		"svelte": "^3.35.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0"
	},
	"dependencies": {
		"@architect/sandbox": "^3.3.7",
		"@begin/data": "^1.2.3"
	},
	"arc": {
		"app": "svelte-app",
		"http": [
			{
				"/api": {
					"method": "get",
					"src": "/api"
				}
			}
		],
		"tables": [
			{
				"data": {
					"scopeID": "*String",
					"dataID": "**String",
					"ttl": "TTL"
				}
			}
		]
	}
}
